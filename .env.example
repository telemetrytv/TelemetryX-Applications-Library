# TelemetryX SDK Configuration
TELEMETRYX_SDK_TOKEN=your_sdk_token_here
TELEMETRYX_API_URL=https://api.telemetryx.ai

# Development Settings
NODE_ENV=development
PORT=3000

# Optional: External API Keys (for development/testing)
# These should be stored in TelemetryX secure storage in production
OPENWEATHER_API_KEY=
YOUTUBE_API_KEY=
SLACK_WEBHOOK_URL=

# Optional: Development Overrides
TELEMETRYX_DEVICE_ID=dev-device-001
TELEMETRYX_LOCATION=40.7128,-74.0060

# Feature Flags
ENABLE_MOCK_DATA=true
ENABLE_DEBUG_LOGGING=false
ENABLE_PERFORMANCE_MONITORING=false