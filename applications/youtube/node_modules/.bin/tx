#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules/@telemetryx/cli/dist/node_modules:/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules/@telemetryx/cli/node_modules:/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules/@telemetryx/node_modules:/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules:/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules/@telemetryx/cli/dist/node_modules:/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules/@telemetryx/cli/node_modules:/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules/@telemetryx/node_modules:/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules:/home/inder/Developer/TelemetryTV/yt-autoplay/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules/@telemetryx/cli/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/@telemetryx+cli@1.0.0-alpha23_@types+node@22.18.6/node_modules/@telemetryx/cli/dist/index.js" "$@"
fi
