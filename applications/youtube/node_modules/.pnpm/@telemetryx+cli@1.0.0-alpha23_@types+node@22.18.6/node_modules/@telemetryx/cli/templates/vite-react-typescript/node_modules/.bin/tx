#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules/@telemetryx/cli/dist/node_modules:/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules/@telemetryx/cli/node_modules:/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules/@telemetryx/node_modules:/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules:/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules/@telemetryx/cli/dist/node_modules:/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules/@telemetryx/cli/node_modules:/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules/@telemetryx/node_modules:/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules:/home/robert/Developer/TelemetryTV/Neo-Application-SDK/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules/@telemetryx/cli/dist/index.js" "$@"
else
  exec node  "$basedir/../../../../../../node_modules/.pnpm/@telemetryx+cli@1.0.0-alpha19_@types+node@22.15.3/node_modules/@telemetryx/cli/dist/index.js" "$@"
fi
